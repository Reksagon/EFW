        <!-- Primary brand color. -->
        <item name="colorPrimary">#EDEDED</item>
        <item name="colorPrimaryVariant">#000000</item>
        <item name="colorOnPrimary">#818181</item>
        <item name="colorAccent">#D2D2D2</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#EDEDED</item>
        <item name="colorSecondaryVariant">#D2D2D2</item>
        <item name="colorOnSecondary">#FBFBFB</item>
        <item name="colorPrimaryDark">#A0A0A0</item>
